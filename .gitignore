# Local LLM Project - Combined .gitignore
# This covers both frontend and backend dependencies and build artifacts

# Dependencies
frontend/node_modules/
backend/node_modules/
node_modules/
/.pnp
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/versions

# Backend model files (large binary files)
backend/models/*.gguf
backend/models/*.bin
backend/models/*.safetensors

# Frontend build outputs
frontend/.next/
frontend/out/
frontend/dist/
frontend/build/

# Backend build outputs
backend/dist/
backend/build/

# Testing
/coverage
frontend/coverage/
backend/coverage/

# Environment variables
.env*
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Runtime and debug files
*.pid
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
*.pem

# IDE files
.vscode/
.idea/
*.swp
*.swo

# TypeScript
*.tsbuildinfo
frontend/next-env.d.ts

# Vercel
.vercel

# Firebase
firebase-debug.log
firestore-debug.log

# Genkit
.genkit/*

# Temporary files
tmp/
temp/